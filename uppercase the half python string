#uppercase the half string in python 


def halfupper(test_str):
    new_str = ''
    hs=len(test_str)//2
    
    for i in range(len(test_str)):
        if i > hs:
            new_str=new_str+test_str[i].upper()
        else:
            new_str+=test_str[i]
    return new_str
    
test_str='kartikkarande'
print(halfupper(test_str))
